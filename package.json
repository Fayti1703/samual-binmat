{
	"name": "binmat",
	"version": "0.0.0",
	"description": "binmat simulator",
	"keywords": [
		"hackmud",
		"binmat"
	],
	"homepage": "https://github.com/samualtnorman/binmat#readme",
	"bugs": {
		"url": "https://github.com/samualtnorman/binmat/issues"
	},
	"license": "GPL-3.0-or-later",
	"author": "Samual Norman",
	"repository": {
		"type": "git",
		"url": "https://github.com/samualtnorman/binmat"
	},
	"scripts": {
		"build": "rollup --config",
		"dev": "rollup --config --watch",
		"lint": "eslint .; tsc",
		"test": "jest --coverage --collectCoverageFrom=\"./src/*.ts\"",
		"package": "rm -rf dist && rollup --config && npm run gen-types && node scripts/build-package-json && cp LICENSE README.md index.cjs dist",
		"push-script": "hsm push game-scripts \"*.binmat\"",
		"gen-types": "tsc --project src"
	},
	"devDependencies": {
		"@babel/core": "^7.18.6",
		"@babel/preset-env": "^7.18.6",
		"@babel/preset-typescript": "^7.18.6",
		"@rollup/plugin-babel": "^5.3.1",
		"@rollup/plugin-commonjs": "^21.1.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@rollup/plugin-typescript": "^8.3.3",
		"@types/jest": "^27.5.2",
		"@types/node": "^17.0.45",
		"@typescript-eslint/eslint-plugin": "^5.30.3",
		"@typescript-eslint/parser": "^5.30.3",
		"eslint": "^8.18.0",
		"eslint-plugin-array-func": "^3.1.7",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-optimize-regex": "^1.2.1",
		"eslint-plugin-regexp": "^1.7.0",
		"eslint-plugin-unicorn": "^41.0.1",
		"hackmud-script-manager": "^0.17.0",
		"jest": "^27.5.1",
		"latest-version": "^6.0.0",
		"rollup": "^2.75.7",
		"rollup-plugin-preserve-shebang": "^1.0.1",
		"rollup-plugin-terser": "^7.0.2",
		"typescript": "^4.7.4"
	},
	"optionalDependencies": {
		"@samual/lib": "^0.3.4",
		"deasync": "^0.1.26"
	},
	"engines": {
		"node": "^12.20 || >=14"
	},
	"private": true,
	"types": "index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"import": "./index.js",
			"require": "./index.cjs"
		},
		"./*": "./*"
	}
}
