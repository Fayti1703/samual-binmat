{
	"name": "binmat",
	"version": "0.0.0",
	"description": "binmat simulator",
	"keywords": [
		"hackmud",
		"binmat"
	],
	"homepage": "https://github.com/samualtnorman/binmat#readme",
	"bugs": {
		"url": "https://github.com/samualtnorman/binmat/issues"
	},
	"license": "GPL-3.0-or-later",
	"author": "Samual Norman",
	"repository": {
		"type": "git",
		"url": "https://github.com/samualtnorman/binmat"
	},
	"scripts": {
		"build": "rollup --config",
		"dev": "rollup --config --watch",
		"lint": "eslint .; tsc --noEmit --emitDeclarationOnly false --project src",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest",
		"package": "rm -rf dist && rollup --config && tsc --project src && node scripts/build-package-json && cp LICENSE package-lock.json README.md dist"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.16.7",
		"@babel/preset-typescript": "^7.16.7",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^21.0.1",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@rollup/plugin-typescript": "^8.3.0",
		"@samual/lib": "^0.2.0",
		"@types/jest": "^27.4.0",
		"@types/node": "^17.0.8",
		"@typescript-eslint/eslint-plugin": "^5.9.0",
		"@typescript-eslint/parser": "^5.9.0",
		"eslint": "^8.6.0",
		"eslint-plugin-array-func": "^3.1.7",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-optimize-regex": "^1.2.1",
		"eslint-plugin-regexp": "^1.5.1",
		"eslint-plugin-unicorn": "^40.0.0",
		"hackmud-script-manager": "^0.13.0-7dddbef",
		"jest": "^27.4.7",
		"rollup": "^2.63.0",
		"rollup-plugin-preserve-shebang": "^1.0.1",
		"rollup-plugin-terser": "^7.0.2",
		"typescript": "^4.5.4"
	},
	"optionalDependencies": {
		"deasync": "^0.1.24"
	},
	"engines": {
		"node": "^12.20 || >=14"
	},
	"private": true,
	"types": "index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"import": "./index.js",
			"require": "./index.cjs"
		},
		"./*": "./*"
	}
}
